class Solution {
    String removeDuplicates(String str) {
 
      
       Set<Character> set=new LinkedHashSet<>();
       
       for(int i=0;i<str.length();i++){
           
           set.add(str.charAt(i));
       }

       String res="";
       
       for(Character element:set){
           
           String c=Character.toString(element);
           
           res+=c;
       }
    
    
      return res;
    }
}
