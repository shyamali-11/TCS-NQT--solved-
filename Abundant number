public class Main
{
	public static void main(String[] args) {
		
		int n=18;
		
		
		int sum=0;
		
		for(int i=2;i*i<=n;i++){
		    if(n%i==0){
		         sum+=i;
		   
		    
		    if(n/i!=i&&n/i!=n)
		     sum+=(n/i);
		    }
		     
		   
		}
		

		
		if(sum+1>n)
		System.out.println("yes");
		
		else
		System.out.println("No");
	}
}
