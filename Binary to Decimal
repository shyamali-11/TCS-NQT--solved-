class Solution {
    public int binary_to_decimal(String str) {
      
        
        int n=0;
       
       int k=0;
      
      for(int j=str.length()-1;j>=0;j--){
          
          int pow=(int)Math.pow(2,k);
          k++;
          
          int val=str.charAt(j)-'0';
          
           n=n+(val*pow);

        
          
          
      }
        return n;
        
        
    }
}
