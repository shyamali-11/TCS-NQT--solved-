class GfG {
    int convertfive(int num) {
     
        
        String str=Integer.toString(num);
        
        String res="";
        
        for(int i=0;i<str.length();i++){
      
            if(str.charAt(i)=='0')
             res=res+"5";
             
             
            else
            res=res+(Character.toString(str.charAt(i)));
        }
        
        return Integer.parseInt(res);
    }
}
