public class Main
{
	public static void main(String[] args) {
		
		int n=23004;
		
		int max=Integer.MIN_VALUE;
		
		int min=Integer.MAX_VALUE;
		
		while(n>0){
		    
		  int  r=n%10;
		    
		    max=Math.max(max,r);
		    
		    min=Math.min(min,r);
		    
		    n=n/10;
		}
		
		System.out.println("Max : "+max);
		System.out.println("Min : "+min);
	}
}
