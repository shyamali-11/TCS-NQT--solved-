//Time Complexity=O(n^2)
//Space Complexity=O(1);
//Stable Algorithm


class Solution
{
    
	public static void bubbleSort(int a[], int n)
    {
       
       for(int i=0;i<n;i++){
           for(int j=0;j<n-1;j++){
               if(a[j]>a[j+1]){
                   int temp=a[j];
                   a[j]=a[j+1];
                   a[j+1]=temp;
               }
           }
       }
    }
}
